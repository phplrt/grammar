{
    "name": "phplrt/grammar",
    "type": "library",
    "description": "The phplrt grammar package.",
    "license": "MIT",
    "keywords": ["parser", "grammar", "rules", "asserts"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/grammar"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "phplrt/grammar-contracts": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Grammar\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Phplrt\\Grammar\\Tests\\": "tests"
        }
    },
    "provide": {
        "phplrt/grammar-contracts-implementation": "^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
